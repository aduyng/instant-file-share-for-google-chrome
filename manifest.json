{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "Instance File Share for Google Drive(TM)",
    "key": "",
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "Instance File Share for Google Drive(TM)"
//        ,
//        "default_popup": "popup/popup.html"
    },
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "permissions":[
        "https://*.google.com/*",
        "tabs"
    ],
    "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}